{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    // "linebreak-style": ["error", (process.platform === "win32" ? "windows" : "unix")],
    "linebreak-style": ["error", "windows"],
    "arrow-body-style":0,
    "no-unused-vars":0,
    "no-console":0
  }
}
